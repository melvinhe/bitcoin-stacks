	(unwrap-panic (as-contract (contract-call? 'SP229FR0MTFR0PX83YS9P5KEHAFPPKTXPG04RKP7T.afro-king transfer u238 tx-sender agent-4)))
	(unwrap-panic (as-contract (contract-call? 'SP3M7N9Q9HDRM7RVP1Q26P0EE69358PZZAX4MD19Q.nft-nyc-exclusive transfer u81 tx-sender agent-4)))
	(unwrap-panic (begin
